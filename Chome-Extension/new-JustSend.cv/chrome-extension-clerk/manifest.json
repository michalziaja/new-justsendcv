{
    "manifest_version": 3,
    "name": "Clerk Auth Status",
    "version": "1.0",
    "action": {
      "default_popup": "build/index.html"
    },
    "permissions": [
      "storage",
      "identity"
    ],
    "host_permissions": [
      "http://localhost:3000/*"
    ],
    "background": {
      "service_worker": "build/background.js"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
  }
  