{
  "manifest_version": 3,
  "name": "My Next.js Clerk Extension",
  "version": "1.0",
  "description": "Sprawdza status logowania w aplikacji Next.js z Clerk",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://*.pracuj.pl/*",
    "https://*.linkedin.com/*",
    "https://*.indeed.com/*",
    "https://*.rocketjobs.pl/*",
    "https://*.nofluffjobs.com/*",
    "https://*.gowork.pl/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
      
  }
}